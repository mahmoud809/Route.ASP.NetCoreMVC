@model int

<td>
    <a  asp-action="Details" asp-route-id="@Model" class="btn btn-outline-warning">
        <i class="fa-solid fa-eye"></i> Details
    </a>
</td>
<td>
    <a asp-action="Edit" asp-route-id="@Model" class="btn btn-outline-success">
        <i class="fa-solid fa-pen-to-square"></i> Update
    </a>
</td>
<td>
    <a asp-action="Delete" asp-route-id="@Model" class="btn btn-outline-danger">
        <i class="fa-solid fa-trash"></i> Delete
    </a>
</td>